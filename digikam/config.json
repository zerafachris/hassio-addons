{
  "apparmor": true,
  "arch": ["aarch64", "amd64", "armv7", "armhf"],
  "boot": "auto",
  "description": "Digikam is a Professional Photo Management with the Power of Open Source",
  "devices": [
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2"
  ],
  "environment": {
    "PUID": "0",
    "PGID": "0",
    "TZ":"Europe/Paris"
  },
  "map": ["config:rw", "share:rw", "media:rw", "ssl"],
  "name": "digikam",
  "options": {
    "PUID": 0,
    "PGID": 0,
    "TZ":"Europe/Paris"
  },
  "ports": {
    "3000/tcp": 3000
  },
  "ports_description": {
    "3000/tcp": "web interface"
  },
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH"],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  },
  "slug": "digikam",
  "startup": "services",
  "upstream": "3.2.2.5080",
  "url": "https://github.com/zerafachris/hassio-addons",
  "version": "0.0.0.1",
  "webui": "http://[HOST]:[PORT:3000]"
}
